<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

<div class="ui ten wide column">
<%= f.input_field :email, required: true, autofocus: true, class: "form-control", placeholder: "Email" %>
<%= f.input_field :password, required: true, class: "form-control", placeholder: "Password" %>
<%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>
<%= f.button :submit, "Sign In", class: 'btn btn-primary btn-block btn-flat' %>

<%= render "devise/shared/links" %>
<% end %>
</div>
